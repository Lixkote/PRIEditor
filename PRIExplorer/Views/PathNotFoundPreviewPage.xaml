<?xml version="1.0" encoding="utf-8"?>
<Page x:Class="PRIExplorer.Views.PathNotFoundPreviewPage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:PRIExplorer.Views" Title="PathNotFoundPreviewPage">
  <StackPanel Margin="8">
    <TextBlock Margin="0,0,0,8" Text="{Binding Message}" />
    <Button Name="setResourceRootPathButton" Content="Set resource root path..." HorizontalAlignment="Left" Command="{Binding SetResourceRootPathCommand}" />
  </StackPanel>
</Page>